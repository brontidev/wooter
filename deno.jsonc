{
	"workspace": {
		"members": ["./middleware", "./wooter"]
	},
	"tasks": {
		"check": "deno fmt --check && deno lint && deno publish --dry-run --allow-dirty",
		"fix": "deno fmt && deno lint --fix && deno publish --dry-run --allow-dirty"
	},
	"fmt": {
		"semiColons": false,
		"useTabs": true,
		"indentWidth": 4
	}
}
